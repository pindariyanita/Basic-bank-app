<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.basicbankapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BasicBankApp">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PassBook" />
        <activity android:name=".Payment_Receipt" />
        <activity android:name=".Transaction" />
        <activity android:name=".SelectUser" />
        <activity android:name=".UserDetails" />
        <activity android:name=".AllUser" />
        <activity android:name=".MainActivity">

        </activity>
    </application>

</manifest>